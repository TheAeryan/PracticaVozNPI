<vxml version="2.1" xml:lang="es-ES">
	<form id="prueba_sumit" scope="document">
		<var name="datos" expr="13"/>
		<field name="hacer_submit"> 
			<grammar>submit | si</grammar>

			<filled>
				<prompt>Submit.</prompt>
				<submit next="http://example.net" namelist="datos"/>
